{
    "manifestVersion": 1,
    "id": "githubpages-publish",
    "version": "1.0.0",
    "name": "GitHub Pages Publish",
    "public": true,
    "description": "Publishes documentation to GitHub Pages",
    "categories": [
        "Build and release"
    ],
    "publisher": "AccidentalFish",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
            }
        ],
    "icons": {
        "default": "icon.png"
     },
     "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "tags": [
        "GitHub",
        "GitHub Pages",
        "Pages",
        "Documentation",
        "Release",
        "Integration",
        "Publish"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/JamesRandall/Vsts-GitHub-Pages-Publish/blob/master/README.md"
        },
        "support": {
            "uri": "https://github.com/JamesRandall/Vsts-GitHub-Pages-Publish/issues"
        }
    },
    "contributions": [
        {
            "id": "AccidentalFish.GitHubPages-Publish",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ]
}